{
  "name": "simple-rag-chatbot",
  "version": "0.1.0",
  "description": "A CLI-based RAG chatbot without authorization restrictions",
  "main": "cli.js",
  "type": "module",
  "scripts": {
    "start": "node cli.js start",
    "initialize": "npx prisma generate && node cli.js initialize",
    "initialize:notion": "npx prisma generate && node cli.js initialize-notion",
    "sync:notion": "node scripts/sync-notion.js",
    "prisma:generate": "npx prisma generate",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "rag",
    "chatbot",
    "llm",
    "embeddings",
    "retrieval"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@google/generative-ai": "^0.2.1",
    "@notionhq/client": "^2.2.14",
    "@prisma/client": "^5.5.2",
    "commander": "^11.1.0",
    "debug": "^4.3.4",
    "dotenv": "^16.3.1",
    "inquirer": "^9.2.12",
    "js-yaml": "^4.1.0"
  },
  "devDependencies": {
    "prisma": "^5.5.2",
    "supabase": "^1.112.0"
  }
}
